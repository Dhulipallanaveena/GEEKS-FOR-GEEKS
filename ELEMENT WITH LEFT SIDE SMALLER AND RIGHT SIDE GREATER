def findElement( arr, n):

    prev=arr[0]

    pos=0
    d_s=0
    i_s=0
    for i in range(1, len(arr)):

        if(prev<=arr[i] and d_s==0):
            prev=arr[i]
            d_s=1
        elif(prev>arr[i] and d_s==1):
            d_s=0
            i_s=0

        elif(prev<=arr[i] and d_s==1):
            i_s=1
            pos=prev

    if(d_s==1 and i_s==1):
        return prev
    else:
        return(-1)
    
